                                            "type": "integer"
                                        },
                                        "minItems": 2,
                                        "maxItems": 2
                                    },
                                    "required_planet_bonuses": {
                                        "type": "array",
                                        "items": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "unevaluatedProperties": false
                            },
                            "name": {
                                "type": "string"
                            }
                        },
                        "unevaluatedProperties": false
                    }
                },
                "gravity_well_fillings": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "filling": {
                                "type": "object",
                                "properties": {
                                    "sockets": {
                                        "type": "object",
                                        "properties": {
                                            "angle": {
                                                "$ref": "#/$defs/float_range"
                                            },
                                            "distance_as_percent_of_parent_inner_move_radius": {
                                                "$ref": "#/$defs/float_range"
                                            },
                                            "socket_children": {
                                                "type": "array",
