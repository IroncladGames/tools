                "random_gravity_well_fillings": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "filling": {
                                "type": "object",
                                "properties": {
                                    "items": {
                                        "type": "array",
                                        "items": {
                                            "type": "object",
                                            "properties": {
                                                "name": {
                                                    "type": "string"
                                                },
                                                "probability": {
                                                    "type": "number"
                                                }
                                            },
                                            "unevaluatedProperties": false
                                        }
                                    }
                                },
                                "unevaluatedProperties": false
                            },
                            "name": {
                                "type": "string"
                            }
                        },
                        "unevaluatedProperties": false
                    }
