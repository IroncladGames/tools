                        "unevaluatedProperties": false
                    }
                },
                "npc_fillings": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "filling": {
                                "type": "object",
                                "properties": {
                                    "npc_filling_type": {
                                        "type": "string",
                                        "enum": [
                                            "militia",
                                            "guardian",
                                            "enemy_faction",
                                            "friendly_faction"
