                        "unevaluatedProperties": false
                    }
                },
                "moon_fillings": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "filling": {
                                "type": "object",
                                "properties": {
                                    "moons": {
                                        "type": "array",
