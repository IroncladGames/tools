                                            "unevaluatedProperties": false
                                        }
                                    }
                                },
                                "unevaluatedProperties": false
                            },
                            "name": {
                                "type": "string"
                            }
                        },
                        "unevaluatedProperties": false
                    }
                },
                "node_fillings": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "filling": {
                                "type": "object",
                                "properties": {
                                    "editor_color": {
                                        "type": "string"
                                    },
                                    "editor_visual_tag": {
                                        "type": "string"
                                    },
                                    "gravity_wells": {
                                        "type": "string"
                                    },
                                    "is_existence_known_to_all": {
                                        "type": "boolean"
                                    },
                                    "is_primary_fixture_player_home_planet": {
                                        "type": "boolean"
                                    },
                                    "override_militia_supply": {
                                        "type": "string"
                                    },
                                    "primary_random_fixture_filling_name": {
                                        "type": "string"
