                                                "chance_of_loot": {
                                                    "type": "number"
                                                },
                                                "chance_of_retrograde_orbit": {
                                                    "type": "number"
                                                },
                                                "chance_of_second_planet_bonus": {
                                                    "type": "number"
                                                },
                                                "counts": {
                                                    "type": "array",
                                                    "items": {
                                                        "type": "object",
                                                        "properties": {
                                                            "count": {
                                                                "type": "integer"
                                                            },
                                                            "probability": {
                                                                "type": "number"
                                                            }
                                                        },
                                                        "unevaluatedProperties": false
                                                    }
                                                },
                                                "filling_name": {
                                                    "type": "string"
                                                },
                                                "ignore_orbit_overlap_checks": {
                                                    "type": "boolean"
                                                },
                                                "inherit_original_parent_orbit_direction": {
                                                    "type": "boolean"
